	/* misc */
	.gallery_button {
		align-self: center;
		margin: auto;
		padding: 40px 30px 50px 30px;
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;

	}

	.gallery_button p {
		color: #eeeeee;
		padding: 0;
		margin: 0;

		transition: 0.2s linear;
	}

	.gallery_button:hover p {
		color: #ffffff;
		padding-top: 0.5rem;

		transition: 0.2s linear;
	}

	.arrow_container {
		width: 0;
		height: 0;
		padding: 0;
		margin: 5px 0 0 0;
	}

	.arrow {
		height: 2rem;
		width: 2rem;
		transform: translateY(-50%) translateX(-50%) rotate(45deg);
		border-bottom: 5px solid var(--sec-fg-color);
		border-right: 5px solid var(--sec-fg-color);

		transition: 0.2s linear;
	}

	.gallery_button:hover .arrow {
		transform: translateY(-25%) translateX(-50%) rotate(45deg);
		border-bottom: 5px solid var(--main-fg-color);
		border-right: 5px solid var(--main-fg-color);

		transition: 0.2s linear;
	}

	.insta {
		display: flex;
		flex-direction: row;
		align-items: center;
		justify-content: flex-end;
	}

	.insta::before {
		opacity: 0.6;
		margin-right: 3px;
		content: '';
		background-image: url('/web-2/includes/assets/images/insta.png'), url('/includes/assets/images/insta.png');
		/* fuuuuuuuuudsugovuoghrubfw*/
		background-size: contain;
		background-repeat: no-repeat;
		display: inline-block;
		width: 1rem;
		height: 1rem;
	}

	/* .productPrice{
		display: flex;
		flex-direction: row;
		flex-wrap: wrap;
		align-items: center;
		justify-content: flex-start;
	} */
	.productPrice {
		color: var(--main-fg-color);
	}

	.buyButton {
		margin: 1rem 0;
		position: relative;
		padding-left: 2rem;
	}

	.buyButton::before {
		opacity: 0.6;
		content: ' ';
		background-image: url('/web-2/includes/assets/images/cart.png'), url('/includes/assets/images/cart.png');
		background-size: contain;
		background-repeat: no-repeat;
		display: inline-block;
		position: absolute;
		left: 0;
		top: 50%;
		transform: translateY(-50%);
		width: 1.5rem;
		height: 1.5rem;
	}