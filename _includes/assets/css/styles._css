	@keyframes fade_in {
		0% {
			opacity: 0;
		}

		100% {
			opacity: 1.0;
		}
	}

	.gallery__preview {
		/* flex: 0 1 auto;
		height: 20vh;
		margin: 05px; */
		display: block;
		width: 100%;
		height: 100%;
		cursor: zoom-in;
		outline: 0 solid rgba(255, 255, 255, 0.5);
		transition: 0s;
		object-fit: cover;
		animation: 1s ease-out 0s 1 fade_in;
	}

	/* 
.preview:last-child{
	margin-right: auto;
} */
	.gallery__preview:hover {
		/* transition: ease-out 0.05s;
	outline: 5px solid rgba(255,255,255,0.2); */
		/* animation: zoom ease-out 1s;
	animation-fill-mode: forwards; */
		outline: 10px solid rgba(255, 255, 255, 0);
		transition: ease-out 0.5s;
		/* animation: zoom 0.3s ; */
	}

	.gallery__preview--zoom {
		animation: zoom 0.3s;
	}

	@keyframes zoom {
		0% {
			outline: 0 solid rgba(255, 255, 255, 0.5);
		}

		50% {
			outline: 10px solid rgba(255, 255, 255, 0);
		}

		100% {
			outline: 0 solid rgba(255, 255, 255, 0.5);
		}
	}

