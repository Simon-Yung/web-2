	.controlBar__button {
		background-color: #00000033;
		border: solid 1px #ffffff33;
		flex: 0.20;
		height: 100%;
		display: flex;
		align-items: center;
		justify-content: center;
		cursor: pointer;

		-webkit-user-select: none;
		/* Safari */
		-moz-user-select: none;
		/* Firefox */
		-ms-user-select: none;
		/* IE10+/Edge */
		user-select: none;
		/* Standard */
		transition: ease-in-out 0.5s;
	}

	.controlBar__button:hover {
		background-color: rgba(255, 255, 255, 0.2);
	}

	.controlBar__button img {
		height: 16px;
	}

	.controlBar__button--previous {
		transform: rotateY(180deg)
	}

	.controlBar__button--next {}

	.controlBar__button--close {}

	.spinner {
		display: block;
		z-index: 11;
		position: fixed;
		top: 0%;
		left: 0%;
		width: 100%;
		height: 100%;
		object-fit: scale-down;
		transform-origin: center;
		/* transform: translateX(-50%) translateY(-50%); */
	}

	.spinning {
		animation: 0.6s linear infinite spin;
	}

	.fade {
		animation: 0.5s linear 1 fade;
		;
	}

	@keyframes spin {
		0% {
			transform: rotate(360deg);
		}

		25% {
			transform: rotate(270deg);
		}

		50% {
			transform: rotate(180deg);
		}

		75% {
			transform: rotate(90deg);
		}

		100% {
			transform: rotate(0deg);
		}
	}

	@keyframes fade {
		0% {
			opacity: 1;
		}

		100% {
			opacity: 0;
		}
	}

	/*menu thingy*/
	.modalWindow__contextualMenu {
		z-index: 13;
		position: fixed;
		top: 100%;
		left: 100%;
		display: none;
		background-color: var(--main-bg-color);
		flex-direction: column;
		flex-wrap: nowrap;
	}

	.modalWindow__contextualMenu__element {
		color: var(--main-fg-color);
		padding: 2px 10px 2px 10px;

		cursor: pointer;

		-webkit-user-select: none;
		/* Safari */
		-moz-user-select: none;
		/* Firefox */
		-ms-user-select: none;
		/* IE10+/Edge */
		user-select: none;
		/* Standard */
	}

	.modalWindow__contextualMenu__element:hover {
		color: var(--main-bg-color);
		background-color: var(--main-fg-color);
	}

	/*----------------------landing page----------------------*/
	.landingPage {
		width: 100%;
		/* min-height: 100vh; */
		display: flex;
		align-items: center;
		justify-content: center;
		background-color: var(--main-bg-color);
		/*#f4f1df;*/
	}

	@keyframes titleAnimation {
		0% {
			height: 0rem;
		}

		100% {
			height: 8.5rem;
		}
	}

	.landingPage__title {
		overflow: hidden;
		font: 300 4rem/1.0 'Montserrat', sans-serif;
		color: var(--main-fg-color);
		padding-bottom: 0;
		border-bottom: 3px solid var(--main-fg-color);
		transition: all 0.2s ease-in-out;
		animation: 1s cubic-bezier(0.68, -0.6, 0.32, 1.6) 0s 1 titleAnimation;
		height: 8.5rem;
		text-align: center;
	}

	.landingPage__title:hover {
		font: 300 4rem/1.0 'Montserrat', sans-serif;
		color: var(--accent-color);
		border-bottom: 3px solid var(--accent-color);
		transition: all 0.3s ease-in-out;
	}

	@media screen and (max-width: 600px) {
		@keyframes titleAnimation {
			0% {
				height: 0rem;
			}

			100% {
				height: 6.5rem;
			}
		}

		.landingPage__title {
			overflow: hidden;
			font: 300 3rem/1.0 'Montserrat', sans-serif;
			color: var(--main-fg-color);
			padding-bottom: 0;
			border-bottom: 3px solid var(--main-fg-color);
			transition: all 0.2s ease-in-out;
			animation: 1s cubic-bezier(0.68, -0.6, 0.32, 1.6) 0s 1 titleAnimation;
			height: 6.5rem;
			text-align: center;
		}

		.landingPage__title:hover {
			font: 300 3rem/1.0 'Montserrat', sans-serif;
			color: var(--accent-color);
			border-bottom: 3px solid var(--accent-color);
			transition: all 0.3s ease-in-out;
		}
	}

	/*----------------------home page----------------------*/
	.homePage {
		width: 100%;
		/* min-height: 100vh; */
		display: flex;
		flex-direction: column;
		flex-wrap: nowrap;
		justify-content: flex-start;
		align-items: center;
		background-color: var(--main-bg-color);
		;
		/*#f4f1df;*/
	}

	.chapterList {
		margin: auto;
		display: flex;
		flex-direction: column;
		flex-wrap: nowrap;
		justify-content: flex-start;
		align-items: center;
		padding: 60px 0 60px 0;
	}

	.chapterList__bigChapter {
		font: 300 1.5rem/1.25 'Montserrat', sans-serif;
		color: var(--main-fg-color);
		;
		margin-top: 10px;
		border-bottom: 1px solid var(--main-fg-color);
		transition: all 0.2s ease-in-out;
		text-align: center;
	}

	.chapterList__chapter {
		font: 300 1.25rem/1.25 'Montserrat', sans-serif;
		color: var(--main-fg-color);
		margin-top: 10px;
		border-bottom: 1px solid #e1c69100;
		transition: all 0.2s ease-in-out;
		text-align: center;
		text-transform: uppercase;
	}

	.chapterList__chapter:hover {
		color: var(--main-fg-color);
		border-bottom: 1px solid var(--main-fg-color);
		transition: all 0.5s ease-in-out;
	}

	/*----------------------gallery page----------------------*/
	.galleryPage {
		width: 100%;
		/* min-height: 100vh; */
		display: flex;
		align-items: center;
		justify-content: center;
		background-color: var(--main-bg-color)
			/*#f4f1df;*/
	}

	.galleryPage__introBox {
		padding: 30px 0px;
		width: 100%;
		/* min-height: 100vh; */
		max-width: 950px;
		display: flex;
		justify-content: flex-start;
		flex-direction: column;
	}

	.galleryPage__introBox__background {
		padding: 30px;
		width: 100%;
		display: flex;
		justify-content: flex-start;
		flex-direction: column;
		background-color: var(--translucent-color);
	}

	.galleryPage__introBox__background>p {
		margin-top: 1.25rem;
		margin-bottom: 1.25rem;
		color: var(--main-fg-color);
	}

	.galleryPage__introBox__background>span {
		margin: 1.25rem 0;
		color: var(--main-fg-color);
	}

	.galleryPage__introBox__background>h2 {
		margin-bottom: 1.25rem;
		color: var(--main-fg-color);
	}

	.galleryPage__introBox__background>hr {
		margin: 2rem auto 2rem auto;
	}

	.galleryPage__introBox__background>h2 {
		margin: 1rem 0 1rem 0;
		color: var(--main-fg-color);
		font-size: 3rem;
		letter-spacing: 0.25rem;
	}

	.gallery {
		/* padding: 20px 55px 60px 55px; */
