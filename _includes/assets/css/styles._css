	/*whole website */
	:root {
		--main-bg-color: #121212;
		--sec-bg-color: #333333;

		--sec-fg-color: #adadad;
		--main-fg-color: #ffffff;

		--translucent-color: #ffffff0f;

		--accent-color: #f59631;
		--link-color: #9a6bfd;
		font-size: 14px;
	}

	body {
		margin: 0;
		padding: 0;
		background-color: var(--main-bg-color);
		/*#f4f4f4;*/
		display: flex;
		flex-wrap: nowrap;
		flex-direction: column;
		justify-content: center;
		align-items: center;
	}

	html {
		box-sizing: border-box;
	}

	*,
	*:before,
	*:after {
		box-sizing: inherit;
	}

	/*text formatting*/

	/*  <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@200;300;400;500;600;700&display=swap" rel="stylesheet">  */
	/* font-family: 'Montserrat', sans-serif; */
	/* hr {
	border-top: 1px solid #eeeeee;
} */
	hr {
		width: 60px;
		margin: 0 auto 1.25rem auto;
		height: 1px;
	}

	p,
	b,
	small,
	big,
	h1,
	strong,
	span {
		font-family: 'Montserrat', sans-serif;
		white-space: normal;
		text-indent: 0;
		margin: 0;
		padding: 0;
	}

	a {
		text-decoration: none;
		font: 400 1rem/1.5 'Montserrat', sans-serif;
		color: var(--link-color);
	}

	h1 {
		/* just for video titles */
		font: 200 2rem/1.0 'Montserrat', sans-serif;
		color: var(--main-fg-color);
		padding-bottom: 0rem;
		text-align: left;
		letter-spacing: 0.25rem;
		text-transform: uppercase;
	}

	h2 {
		font: 200 1.5rem/1.0 'Montserrat', sans-serif;
		color: var(--main-fg-color);
		padding-bottom: 0rem;
		letter-spacing: 0.25rem;
		text-align: left;
	}

	p {
		font: 200 1rem/1.5 'Montserrat', sans-serif;
		color: var(--main-fg-color);
	}

	span {
		font: 200 1rem/1.5 'Montserrat', sans-serif;
		color: var(--main-fg-color);
	}

	strong,
	b {
		font: 600 1rem/1.5 'Montserrat', sans-serif;
		color: var(--main-fg-color);
	}

	small {
		font: 300 0.75rem/01.5 'Montserrat', sans-serif;
		color: var(--main-fg-color);
	}

	blockquote {
		margin: 10px;
		border-left: 3px solid rgba(255, 255, 255, 0.1);
		padding-left: 10px;
	}

	body {
		min-height: 100vh;
	}

	/*----------------------components----------------------*/

	/*----------------------backgrounds----------------------*/

	@keyframes fadeIn {
		0% {
			opacity: 0;
		}

		100% {
			opacity: 0.6;
		}
	}

	@keyframes fadeOut {
		0% {
			opacity: 0.6;
		}

		100% {
			opacity: 0;
		}
	}

	.backgroundImage--fadeIn {
		animation: 0.5s ease-in-out 0s 1 normal forwards running fadeIn;
	}

	.backgroundImage--fadeOut {
		animation: 0.5s ease-in-out 0s 1 normal forwards running fadeOut;
	}

	.backgroundImage {
		position: fixed;
		object-fit: cover;
		filter: blur(05px);
		opacity: 0;
		top: -5%;
		left: -5%;
		min-width: 110%;
		z-index: -1;
		height: 110%;
		animation: none;
	}

	.background {
		position: fixed;
		object-fit: cover;
		filter: blur(05px);
		opacity: 0.6;
		top: -5%;
		left: -5%;
		min-width: 110%;
		z-index: -1;
		height: 110%;
		animation: none;
	}

	/*----------------------header----------------------*/
	.header {
		width: 100%;
		min-height: 50px;
		padding: 15px 20px 0 20px;
		margin: 0 auto 0 auto;
		background-color: var(--main-bg-color);
		/* #b7b39b; */

		display: flex;
		flex-direction: column;
		flex-wrap: nowrap;
		justify-content: center;
		align-items: center;
		height: auto;
		border-bottom: 1px solid var(--sec-bg-color);
	}

	.header__inner {
		width: 100%;
		max-width: 1000px;

		display: flex;
		flex-direction: row;
		flex-wrap: wrap;
		justify-content: space-between;
		z-index: 2;
	}

	.header__underBlock {
		padding: 0;
		margin: 0;
		/* height: 0; */
		width: 100%;
		max-width: 1000px;

		display: flex;
		flex-direction: row;
		flex-wrap: nowrap;
		justify-content: flex-end;
	}

	.header__underBlock__text {
		color: inherit;
		/* width: 100%; */
		text-align: right;
		max-width: 1000px;
		color: var(--main-fg-color);
	}

	.header__underBlock__text>a {
		color: var(--sec-fg-color);
	}

	.header__logo {
		height: 100%;
		font: 300 2rem/1 'Montserrat', sans-serif;
		color: var(--main-fg-color);
		;
		display: flex;
		align-items: center;
		justify-content: flex-start;
		padding: 0 2px 0 2px;

		/* border-bottom: 1px solid #b7b39b; */
		transition: all 0.2s ease-in-out;
	}

	.header__logo:hover {
		/* border-bottom: 1px solid #ffffff; */
		color: var(--main-fg-color);
		;
		text-decoration: underline;

		transition: all 0.2s ease-in-out;
	}

	.navbar {
		display: flex;
		flex-direction: row;
		flex-wrap: nowrap;
		justify-content: flex-end;
		align-items: flex-end;
	}

	.navbar a:nth-child(1) span {
		margin-left: 0.25rem;
	}

	.navbar__separator {
		display: inline-block;
		font: 300 1.25rem/1.25 'Montserrat', sans-serif;
		padding: 0 0 0 0;
		margin-left: 5px;
		/* border-bottom: 1px solid #b7b39b; */
		color: var(--main-bg-color);
		transition: all 0.2s ease-in-out;
	}

	.navbar__bigItem {
		font: 300 1.25rem/1.25 'Montserrat', sans-serif;
		padding: 0 2px 0 2px;
		margin-left: 5px;
		color: var(--sec-fg-color);

		transition: all 0.2s ease-in-out;
	}

	.navbar__bigItem:hover {
		color: var(--sec-fg-color);
		text-decoration: underline;

		transition: all 0.5s ease-in-out;
	}

	.navbar__item {
		font: 300 1.25rem/1.25 'Montserrat', sans-serif;
		padding: 0 2px 0 2px;
		margin-left: 5px;
		/* border-bottom: 1px solid #b7b39b; */
		color: var(--main-bg-color);

		transition: all 0.2s ease-in-out;
	}

	.navbar__item:hover {
		color: var(--accent-color);
		text-decoration: underline;

		transition: all 0.5s ease-in-out;
	}

	@media screen and (max-width: 600px) {
		.navbar {
			margin-top: 0.5rem;
			flex-wrap: wrap;
			justify-content: flex-start;
			align-items: flex-start;
		}

		.navbar__separator {
			display: none;
		}

		.navbar__bigItem,
		.navbar__item {
			margin-left: 0;
		}
	}

	/*----------------------footer----------------------*/
	.footer {
		width: 100%;
		min-height: 50px;
		padding: 0 2rem 0 2rem;
		margin: 3rem auto 0 auto;
		/* margin-top: auto; */

		display: flex;
		flex-direction: row;
		flex-wrap: wrap;
		border-top: 1px solid var(--sec-bg-color);
		justify-content: space-between;
		align-items: center;
		background-color: var(--main-bg-color);
		/* #b7b39b; */
	}

	.footer__text {
		margin: auto;
		font: 300 1.25rem/1.25 'Montserrat', sans-serif;
		color: var(--main-fg-color);
	}

	.footer__text a {
		font: 300 1rem/1.25 'Montserrat', sans-serif;
		color: var(--sec-fg-color);
	}

	.footer__text a:hover {
		text-decoration: underline;
	}

	.footer__text:nth-child(1) {
		margin-right: auto;
		display: flex;
		flex-direction: row;
		justify-content: center;
		align-items: center;
		flex-wrap: wrap;
	}

	.footer__text:nth-child(1)>* {}

	.footer__text:nth-child(2) {
		margin-left: auto;
	}

	/*----------------------back to top button----------------------*/
	.backToTop {
		cursor: pointer;

		position: fixed;
		bottom: 20px;
		right: 20px;
		height: 45px;
		width: 45px;

		display: flex;
		align-content: center;
		justify-content: center;
		background-color: var(--main-fg-color);
		opacity: 30%;
	}

	.backToTop__icon {
		display: block;
		height: 25px;
		width: 25px;
		transform: translateY(50%) rotate(225deg);
		border-bottom: 5px solid var(--sec-bg-color);
		border-right: 5px solid var(--sec-bg-color);
	}

	.backToTop:hover {
		opacity: 100%;
		transition: all ease-in-out 0.3s;
	}

	.backToTop:hover .backToTop__icon {
		transition: all ease-in-out 0.3s;
	}

	/*----------------------modal window----------------------*/
	.modalWindow {
		position: fixed;
		top: 0;
		left: 0;

		display: none;
		flex-direction: column;
		flex-wrap: nowrap;
		align-items: center;
		justify-content: center;

		z-index: 10;
		width: 100%;
		height: 100%;
		max-height: 100%;
		/* padding: 20px 20px 20px 20px; doesn't work on google chrome*/
		background-color: rgba(0, 0, 0, 0.8);
	}

	.modalWindow__container {
		/* FUCK YOU GOOGLE CHROME */
		padding: 20px;
		flex: 1 1 auto;

		width: 100%;
		max-height: 100%;
		min-height: 0;

		display: flex;
		justify-content: center;
		align-items: center;

		cursor: zoom-out;
	}

	.modalImage {
		flex: 1 1 auto;

		margin: auto;
		object-fit: scale-down;
		/* because of the old  */

		max-width: 100%;
		max-height: 100%;

		min-width: 0;
		min-height: 0;
	}

	.controlBar {
		margin-bottom: 20px;
		gap: 5px;
		flex: 0 1 auto;
		z-index: 12;
		/* above the spinner */
		background-color: rgba(0, 0, 0, 0.0);
		width: 100%;
		height: auto;

		display: flex;
		flex-direction: row;
		flex-wrap: nowrap;
		align-items: center;
		justify-content: space-evenly;
	}

	.controlBar__pagination {
		background-color: #00000033;
		border: solid 1px #ffffff33;
		flex: 0.60;
		font: 200 16px/1.0 'Montserrat', sans-serif;
		color: var(--main-fg-color);
		padding: 5px;
		text-align: center;
		display: flex;
		flex-direction: row;
		align-items: center;
		justify-content: center;
		transition: ease-in-out 0.5s;
	}

	.controlBar__pagination:hover {
		background-color: rgba(255, 255, 255, 0.2);
	}

	.controlBar__pagination::after {
		margin-left: 6px;
		content: '';
		background-image: url('/web-2/includes/assets/images/outgoing.png'), url('/includes/assets/images/outgoing.png');
		/* fuuuuuuuuudsugovuoghrubfw*/
		background-size: contain;
		background-repeat: no-repeat;
		display: inline-block;
		width: 16px;
		height: 16px;
	}

